{
  "name": "YAPL",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_definition"
          }
        ]
      }
    },
    "_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "declaration_statement"
            },
            {
              "type": "SYMBOL",
              "name": "initialization_statement"
            }
          ]
        }
      ]
    },
    "_declaration": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "simple_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "array_declaration"
        }
      ]
    },
    "_initialization": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "simple_initialization"
        },
        {
          "type": "SYMBOL",
          "name": "array_initialization"
        },
        {
          "type": "SYMBOL",
          "name": "struct_initialization"
        }
      ]
    },
    "_values": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "bool_literal"
        }
      ]
    },
    "declaration_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "semi"
        }
      ]
    },
    "initialization_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_initialization"
        },
        {
          "type": "SYMBOL",
          "name": "semi"
        }
      ]
    },
    "simple_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "type",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        }
      ]
    },
    "array_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "simple_declaration"
        },
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "FIELD",
          "name": "size",
          "content": {
            "type": "SYMBOL",
            "name": "number"
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "simple_initialization": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "simple_declaration"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "_values"
          }
        }
      ]
    },
    "array_initialization": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "array_declaration"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "initializer_list"
        }
      ]
    },
    "struct_initialization": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "struct_constructor_initialization"
        },
        {
          "type": "SYMBOL",
          "name": "struct_list_initialization"
        }
      ]
    },
    "struct_constructor_initialization": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "simple_declaration"
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "FIELD",
          "name": "values",
          "content": {
            "type": "SYMBOL",
            "name": "arguments"
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "struct_list_initialization": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "simple_declaration"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "initializer_list"
        }
      ]
    },
    "initializer_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "FIELD",
          "name": "values",
          "content": {
            "type": "SYMBOL",
            "name": "arguments"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "arguments": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_values"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "_values"
              }
            ]
          }
        }
      ]
    },
    "bool_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "([a-zA-Z]|_)+([a-zA-Z0-9]|_)*"
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "semi": {
      "type": "STRING",
      "value": ";"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

